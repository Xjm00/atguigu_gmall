<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.atguigu.gmall.manage.mapper.PmsBaseAttrInfoMapper" >    <select id="selectBySkuValueIds" resultMap="selectBySkuValueId_resultMap"            parameterType="java.lang.String">      SELECT bai.*, bav.*, bai.id as bai_id, bav.id as bav_id      FROM pms_base_attr_info bai,            pms_base_attr_value bav      WHERE bai.id = bav.attr_id      AND bav.id IN (${join})    </select>    <resultMap id="selectBySkuValueId_resultMap" type="com.atguigu.gmall.bean.PmsBaseAttrInfo"        autoMapping="true">        <id column="bai_id" property="id"/>        <collection property="attrValueList" ofType="com.atguigu.gmall.bean.PmsBaseAttrValue"        autoMapping="true">            <id column="bav_id" property="id"/>        </collection>    </resultMap></mapper>